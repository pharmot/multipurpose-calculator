{
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["."],
    "exclude": ["node_modules", "dist", "docs", "webpack.config.js", "webpack.dev.config.js"]
  },
  "recurseDepth": 4,
  "opts": {
    "destination": "./docs/jsdoc/",
    "recurse": true,
    "template": "jsdoc-template"
  },
  "plugins": ["plugins/markdown"],
  "markdown": {
    "tags": ["author", "classdesc", "description", "exceptions", "params", "properties", "returns", "see", "summary", "example"]
  }
}
